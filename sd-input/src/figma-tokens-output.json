{
  "global": {
    "font-size-m": {
      "value": "18px",
      "type": "fontSizes"
    },
    "line-height-m": {
      "value": "1.5",
      "type": "lineHeights"
    },
    "color-red": {
      "value": "#f03",
      "type": "color"
    },
    "color-blue": {
      "value": "#00cfff",
      "type": "color"
    },
    "font-size-l": {
      "value": "{font-size-m} * {font-size-ratio}",
      "type": "fontSizes"
    },
    "font-size-ratio": {
      "value": "1.618",
      "type": "other"
    },
    "font-size-xl": {
      "value": "{font-size-l} * {font-size-ratio}",
      "type": "fontSizes"
    },
    "font-size-s": {
      "value": "{font-size-m} / {font-size-ratio}",
      "type": "fontSizes"
    },
    "article": {
      "body": {
        "paragraph": {
          "font-size": {
            "value": "{font-size-m}",
            "type": "fontSizes"
          }
        },
        "heading1": {
          "font-size": {
            "value": "{font-size-l}",
            "type": "fontSizes"
          }
        }
      }
    }
  },
  "article": {
    "body-font-size": {
      "value": "{font-size-m}",
      "type": "fontSizes"
    },
    "body-line-height": {
      "value": "{line-height-m}",
      "type": "lineHeights"
    },
    "body-color": {
      "value": "{color-red}",
      "type": "color"
    },
    "body": {
      "paragraph": {
        "value": {
          "lineHeight": "{body-line-height}",
          "fontSize": "{body-font-size}",
          "letterSpacing": "0",
          "paragraphSpacing": "{body-font-size} * {body-line-height}"
        },
        "type": "typography"
      },
      "heading": {
        "typography": {
          "value": {
            "lineHeight": "a"
          },
          "type": "typography"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global",
      "article"
    ]
  }
}