{
  "global": {
    "font-size-m": {
      "value": "18px",
      "type": "fontSizes"
    },
    "line-height-m": {
      "value": "1.5",
      "type": "lineHeights"
    },
    "color-red": {
      "value": "#f03",
      "type": "color"
    },
    "color-blue": {
      "value": "#00cfff",
      "type": "color"
    }
  },
  "article": {
    "body-font-size": {
      "value": "{font-size-m}",
      "type": "fontSizes"
    },
    "body-line-height": {
      "value": "{line-height-m}",
      "type": "lineHeights"
    },
    "body-color": {
      "value": "{color-red}",
      "type": "color"
    },
    "body-typography": {
      "value": {
        "lineHeight": "{body-line-height}",
        "fontSize": "{body-font-size}",
        "letterSpacing": "",
        "paragraphSpacing": "{body-line-height} * 2"
      },
      "type": "typography"
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global",
      "article"
    ]
  }
}