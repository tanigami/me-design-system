{
  "global": {
    "font-size-m": {
      "value": "18px",
      "type": "fontSizes"
    },
    "line-height-m": {
      "value": "1.5",
      "type": "lineHeights"
    },
    "color-red": {
      "value": "#f03",
      "type": "color"
    },
    "color-blue": {
      "value": "#00cfff",
      "type": "color"
    },
    "font-size-l": {
      "value": "{font-size-m} * {font-size-ratio}",
      "type": "fontSizes"
    },
    "font-size-ratio": {
      "value": "1.618",
      "type": "other"
    },
    "font-size-xl": {
      "value": "{font-size-l} * {font-size-ratio}",
      "type": "fontSizes"
    },
    "font-size-s": {
      "value": "{font-size-m} / {font-size-ratio}",
      "type": "fontSizes"
    },
    "article": {
      "body": {
        "paragraph": {
          "font-size": {
            "value": "{font-size-m}",
            "type": "fontSizes"
          },
          "typography": {
            "value": {
              "lineHeight": "{line-height-m}",
              "fontSize": "{font-size-m}",
              "letterSpacing": "0",
              "paragraphSpacing": "{line-height-m} * 2"
            },
            "type": "typography"
          },
          "color": {
            "value": "{color-blue}",
            "type": "color"
          }
        },
        "heading1": {
          "font-size": {
            "value": "{font-size-l}",
            "type": "fontSizes"
          }
        },
        "heading": {
          "typography": {
            "value": {
              "fontSize": "{font-size-l}",
              "lineHeight": "{line-height.m}",
              "paragraphSpacing": "{font-size-l}"
            },
            "type": "typography"
          }
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global"
    ]
  }
}